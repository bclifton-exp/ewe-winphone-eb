				Value="LeftToRight" />
		<Setter Property="FullIconUri"
				Value="\Assets/StarRating/StarRatingFull_Dark.scale-240.png" />
		<Setter Property="EmptyIconUri"
				Value="\Assets/StarRating/StarRatingEmpty_Dark.scale-240.png" />
		<Setter Property="Margin"
				Value="8.3,0,0,0" />
		<Setter Property="Grid.Column"
				Value="1" />
